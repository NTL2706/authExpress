{
  "presets": ["@babel/preset-env"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "configs": "./src/configs",
          "controllers": "./src/controllers",
          "helpers": "./src/helpers",
          "middlewares": "./src/middlewares",
          "models": "./src/models",
          "routes": "./src/routes",
          "services": "./src/services",
          "src": "./src",
          "*": ["*", "./node_modules"]
        },
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    ]
  ]
}
